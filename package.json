{
  "name": "urbanhub-ai-agents",
  "version": "1.0.0",
  "description": "Comprehensive AI agent ecosystem built on Bird.com platform to automate UrbanHub's customer acquisition workflow",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "bird:setup": "tsx scripts/bird-setup.ts",
    "bird:test-webhook": "tsx scripts/test-webhook.ts",
    "bird:deploy-agents": "tsx scripts/deploy-agents.ts",
    "bird:sync-templates": "tsx scripts/sync-templates.ts",
    "agent:test": "tsx scripts/test-agents.ts",
    "agent:validate": "tsx scripts/validate-agents.ts",
    "agent:simulate": "tsx scripts/simulate-conversations.ts",
    "bmad:init": "tsx scripts/bmad-init.ts",
    "bmad:story": "tsx scripts/bmad-story.ts",
    "bmad:architect": "tsx scripts/bmad-architect.ts",
    "hubspot:sync": "tsx scripts/hubspot-sync.ts",
    "hubspot:test": "tsx scripts/test-hubspot.ts",
    "calendar:sync": "tsx scripts/calendar-sync.ts",
    "db:migrate": "tsx scripts/db-migrate.ts",
    "db:seed": "tsx scripts/db-seed.ts",
    "webhook:test": "tsx scripts/webhook-test.ts",
    "deploy:demo": "tsx scripts/deploy-demo.ts",
    "deploy:production": "tsx scripts/deploy-production.ts"
  },
  "keywords": [
    "ai",
    "chatbot",
    "real-estate",
    "whatsapp",
    "hubspot",
    "bird.com",
    "mexico",
    "bmad-method"
  ],
  "author": "UrbanHub AI Team",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@hubspot/api-client": "^11.1.0",
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "bull": "^4.12.2",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "crypto-js": "^4.2.0",
    "moment-timezone": "^0.5.43",
    "googleapis": "^128.0.0",
    "node-fetch": "^3.3.2",
    "ws": "^8.14.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "pdf-parse": "^1.1.1",
    "openai": "^4.20.1",
    "whatsapp-web.js": "^1.23.0",
    "twilio": "^4.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/morgan": "^1.9.7",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.6",
    "@types/crypto-js": "^4.2.1",
    "@types/ws": "^8.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.5",
    "@types/multer": "^1.4.9",
    "@types/jest": "^29.5.7",
    "@types/supertest": "^2.0.15",
    "typescript": "^5.2.2",
    "tsx": "^3.14.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "prettier": "^3.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run typecheck && npm run test"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/urbanhub/ai-agents.git"
  },
  "bugs": {
    "url": "https://github.com/urbanhub/ai-agents/issues"
  },
  "homepage": "https://github.com/urbanhub/ai-agents#readme"
}